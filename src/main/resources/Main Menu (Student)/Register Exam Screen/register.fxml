<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.RegisterExamController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="780.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@background.png" />
         </image>
      </ImageView>
      <ComboBox fx:id="examsDropdown" layoutX="243.0" layoutY="277.0" prefHeight="25.0" prefWidth="199.0" promptText="Exams" />
      <Button fx:id="selectExamBtn" layoutX="454.0" layoutY="277.0" mnemonicParsing="false" onAction="#selectExamClicked" text=" Select Exam" />
      <Button layoutX="375.0" layoutY="342.0" mnemonicParsing="false" text="Register" />
   </children>
</AnchorPane>
