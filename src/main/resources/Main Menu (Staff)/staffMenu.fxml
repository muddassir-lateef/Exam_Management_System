<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderpane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StaffMenuController">
   <left>
      <AnchorPane prefHeight="700.0" prefWidth="220.0" BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="10.0" layoutY="230.0" prefHeight="240.0" prefWidth="200.0">
               <children>
                  <Button fx:id="addExamBtn" mnemonicParsing="false" onAction="#AddExamsClicked" prefHeight="0.0" prefWidth="160.0" text="Add Exams">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="addVenueBtn" mnemonicParsing="false" onAction="#AddVenueClicked" prefHeight="0.0" prefWidth="160.0" text="Add Venue">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>

                  <Button fx:id="scheduleExamBtn" mnemonicParsing="false" onAction="#ScheduleExamsClicked" prefHeight="0.0" prefWidth="160.0" text="Schedule Exams">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="assignPersonelBtn" mnemonicParsing="false" onAction="#AssignPersonnelClicked" prefHeight="0.0" prefWidth="160.0" text="Assign Personnel">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="viewFeedbackBtn" mnemonicParsing="false" onAction="#viewFeedbackClicked" prefHeight="0.0" prefWidth="160.0" text="View Exam Feedback">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="viewCodesBtn" mnemonicParsing="false" onAction="#viewCodesClicked" prefHeight="0.0" prefWidth="160.0" text="View Registration Codes">
                     <VBox.margin>
                        <Insets left="30.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <ImageView fx:id="logOutBtn" fitHeight="48.0" fitWidth="49.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#logOutBtnClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logOut.png" />
               </image>
            </ImageView>


         </children></AnchorPane>
   </left>
</BorderPane>
